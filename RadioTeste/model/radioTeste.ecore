    <eStructuralFeatures xsi:type="ecore:EReference" name="securityLevel" lowerBound="1"
        eType="#//Security" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="SampleValue" lowerBound="1"
